
        // Sample movie data
        const movies = [
            { title: "Inception", genre: "Sci-Fi, Action", rating: 8.8, image: "data:image/jpeg;base64," },
            { title: "The Shawshank Redemption", genre: "Drama", rating: 9.3, image: "data:image/jpeg;base64," },
            { title: "The Dark Knight", genre: "Action, Crime, Drama", rating: 9.0, image: "data:image/jpeg;base64," },
            { title: "PulpFiction ", genre: "Crime, Drama", rating: 8.9, image: "data:image/webp;base64," },
            { title: "Forrest Gump", genre: "Drama, Romance", rating: 8.8, image: "data:image/jpeg;base64," },
            { title: "The Matrix", genre: "Action, Sci-Fi", rating: 8.7, image: "data:image/jpeg;base64," },
            { title: "Goodfellas", genre: "Biography, Crime, Drama", rating: 8.7, image: "data:image/jpeg;base64," },
            { title: "The Silence of the Lambs", genre: "Crime, Drama, Thriller", rating: 8.6, image: "data:image/jpeg;base64," }
        ];

        // Function to create a movie card
        function createMovieCard(movie) {
            return `
                <div class="bg-white rounded-lg shadow-md overflow-hidden">
                    <img src="${movie.image}" alt="${movie.title}" class="w-full h-64 object-cover">
                    <div class="p-4">
                        <h3 class="font-bold text-xl mb-2">${movie.title}</h3>
                        <p class="text-gray-700 text-base mb-2">${movie.genre}</p>
                        <div class="flex items-center">
                            <i class="fas fa-star text-yellow-500 mr-1"></i>
                            <span>${movie.rating}/10</span>
                        </div>
                    </div>
                </div>
            `;
        }

        // Function to render movies
        function renderMovies(moviesToRender) {
            const movieGrid = document.getElementById('movie-grid');
            movieGrid.innerHTML = moviesToRender.map(createMovieCard).join('');
        }

        // Initial render
        renderMovies(movies);

        // Search functionality
        const searchInput = document.getElementById('search');
        searchInput.addEventListener('input', (e) => {
            const searchTerm = e.target.value.toLowerCase();
            const filteredMovies = movies.filter(movie => 
                movie.title.toLowerCase().includes(searchTerm) || 
                movie.genre.toLowerCase().includes(searchTerm)
            );
            renderMovies(filteredMovies);
        });

        // Mobile menu toggle
        const menuToggle = document.getElementById('menu-toggle');
        const menu = document.getElementById('menu');
        menuToggle.addEventListener('click', () => {
            menu.classList.toggle('hidden');
        });
    